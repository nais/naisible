{
  "CN": "kube-apiserver",
  "hosts": [
    "127.0.0.1",
    "{{ kubernetes_default_ip }}",
    "kubernetes",
    "kubernetes.default",
    "kubernetes.default.svc",
    "kubernetes.default.svc.{{ cluster_domain }}",
    "*.{{ domain }}",
    "{{ groups['masters'][0] }}"
  ],
  "key": {
    "algo": "rsa",
    "size": 2048
  }
}
