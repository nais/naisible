apiVersion: audit.k8s.io/v1beta1
kind: Policy
rules:
  - level: None
    nonResourceURLs:
      - '/healthz*'
      - /version
      - '/swagger*'
      - '/openapi/*'
  - level: None
    resources:
      - group: ""
        resources:
          - events
  - level: Metadata
    omitStages:
      - "ResponseStarted"
      - "ResponseComplete"
  - level: None
